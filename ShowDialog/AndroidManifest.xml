<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.telenav.demo.showdialog"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="11"
        android:targetSdkVersion="18" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.telenav.demo.showdialog.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity 
            android:name="com.telenav.demo.showdialog.ManagedDialogActivity"
            android:label="@string/managed_dialog" />
        <activity 
            android:name="com.telenav.demo.showdialog.UnmanagedDialogActivity"
            android:label="@string/unmanaged_dialog" />
        <activity 
            android:name="com.telenav.demo.showdialog.DialogFragmentActivity"
            android:label="@string/dialog_fragment" />
    </application>

</manifest>